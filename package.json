{
  "name": "typeorm-sample",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.19",
    "ts-node": "10.7.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.7",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "serv": "npx nodemon src/index.ts",
    "lint": "eslint . -c .eslintrc.js --ext .ts --ignore-path .eslintignore",
    "lint:fix": "eslint . -c .eslintrc.js --ext .ts --ignore-path .eslintignore --fix",
    "typeorm-cli": "typeorm-ts-node-esm -d ./src/db/dataSource.ts",
    "db:sync": "npm run typeorm-cli -- migration:generate -p src/db/migrations/sync",
    "db:migrate:up": "npm run typeorm-cli -- migration:run",
    "db:migrate:down": "npm run typeorm-cli -- migration:revert"
  }
}
